{% extends "oaipmh/base.xml" %}

{% block content %}
  <ListIdentifiers>
    {% for i in items %}
    <header>
        <identifier>{{ i.identifier }}</identifier>
        <datestamp>{{ i.last_modified|date:'c' }}</datestamp>
        {% for set in i.sets %}
        <setSpec>{{ set }}</setSpec>
        {% endfor %}
    </header>
    {% endfor %}
      <resumptionToken completeListSize="{{ list_size }}"
                       cursor="{{ cursor }}">{{ resumption_token }}</resumptionToken>
  </ListIdentifiers>
{% endblock %}
